{
  "name": "Corporate-UI",
  "version": "0.0.1",
  "description": "In this repository you'll find all components available in the UX Library",
  "module": "tmp/esm/index.js",
  "main": "tmp/index.js",
  "unpkg": "tmp/ionic.js",
  "types": "tmp/types/components.d.ts",
  "collection": "tmp/components/collection-manifest.json",
  "files": [
    "tmp/",
    "dist/"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "start": "gulp",
    "build": "gulp build",
    "themes": "gulp themes",
    "docs": "stencil build --docs",
    "lint": "tslint {*,src/**/*,.storybook/**/*}.{js,jsx,ts,tsx}",
    "lint-fix": "npm run lint -- --fix",
    "prettify": "prettier {*,src/**/*,.storybook/**/*}.{js,jsx,ts,tsx,json,css,scss,md,html} --write",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "husky": {
    "hooks": {
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md,html}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}": [
      "tslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@stencil/core": "0.18.0",
    "@stencil/redux": "0.0.4",
    "@stencil/sass": "^0.1.1",
    "@storybook/addon-actions": "^4.1.11",
    "@storybook/addon-links": "^4.1.11",
    "@storybook/addon-options": "^4.1.11",
    "@storybook/html": "^4.1.13",
    "@types/jest": "23.3.14",
    "@types/puppeteer": "1.12.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "bootstrap": "^4.3.1",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.0",
    "del": "^4.0.0",
    "gulp": "^4.0.0",
    "gulp-copy": "^4.0.1",
    "html-loader": "^0.5.5",
    "http-proxy-middleware": "^0.19.1",
    "husky": "^1.3.1",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "lint-staged": "^8.1.4",
    "marked": "^0.6.0",
    "node-sass": "^4.11.0",
    "prettier": "1.16.4",
    "puppeteer": "1.11.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "redux-watch": "^1.1.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "url-loader": "^1.1.2",
    "webpack": "4.28.4",
    "webpack-cli": "^3.2.3"
  },
  "license": "MIT",
  "dependencies": {}
}
